<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:CompClubGUI"
             x:Class="CompClubGUI.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.Styles>
		<StyleInclude Source="avares://CompClubGUICore/Views/Styles/ControlStyles.axaml" />
		<FluentTheme />
    </Application.Styles>
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="avares://CompClubGUICore/Views/Styles/AppColors.axaml"/>
				<ResourceInclude Source="avares://CompClubGUICore/Views/Styles/AppImages.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>